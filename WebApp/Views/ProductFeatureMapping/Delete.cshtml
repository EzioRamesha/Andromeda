@model WebApp.Models.TreatyBenefitCodeMappingViewModel

@{
    ViewBag.Title = "Product Feature Mapping";
    ViewBag.Action = "Delete";
    ViewBag.ListPageUrl = Url.Action("Index");
}

@section breadcrumb
{
    @Html.Partial("_Breadcrumb")
}

@Html.Partial("_BackToList")

