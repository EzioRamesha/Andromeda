                            <td>@Shared.Util.GetTruncatedValue(Model.CampaignCode)</td>
                        </tr>
                        <tr>
                            <td>@Html.DisplayNameFor(model => model.ReinsEffDatePolStartDate)</td>
                            <td>@Model.ReinsEffDatePolStartDate.Value.ToString(Shared.Util.GetDateFormat())</td>
                        </tr>
                        <tr>
