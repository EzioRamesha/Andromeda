                                </td>
                                <td>
                                    @item.RetroParty2
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroShare2)
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroRecovery2, 2)
                                </td>
                                <td>
                                    @item.RetroParty3
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroShare3)
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroRecovery3, 2)
                                </td>
                            </tr>
                        }
                    }
                    else
                    {
                        <tr>
                            <td colspan="20" class="no-data">No available data found.</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
        @if (ClaimRegisterList != null)
        {
            {
                ViewBag.CurrentPageNumber = ClaimRegisterList.PageNumber;
                ViewBag.CurrentPageItems = ClaimRegisterList.Count;
                ViewBag.PageSize = ClaimRegisterList.PageSize;
                ViewBag.Total = ViewBag.ClaimRegisterTotal;
            }
            <div class="pagination-footer position-relative">
                <div class="row">
                    <div class="col-md-4">
                        @Html.Partial("_PageInfo")
                    </div>
                    <div class="col-md-8">
                        @Html.PagedListPager(ClaimRegisterList, Page => Url.Action("Edit", new
                        {
                            RiDataPage = ViewBag.RiDataPage,
                            ClaimRegisterPage = Page,
                            TabIndex = 2
                        }), Shared.Html.GetPagedListRenderOptions())
                    </div>
                </div>
            </div>
        }
    </div>

