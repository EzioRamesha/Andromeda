</div>

<script>
    // Models
    var ModelId = "@Model.Id";
    var DirectRetroModel = "@Model";

    // Retro Status
    var RetroStatusPending = '@BusinessObject.DirectRetroBo.RetroStatusPending';
    var RetroStatusSubmitForProcessing = '@BusinessObject.DirectRetroBo.RetroStatusSubmitForProcessing';
    var RetroStatusPendingApproval = '@BusinessObject.DirectRetroBo.RetroStatusPendingApproval';
    var RetroStatusApproved = '@BusinessObject.DirectRetroBo.RetroStatusApproved';
    var RetroStatusProcessing = '@BusinessObject.DirectRetroBo.RetroStatusProcessing';
    var RetroStatusCompleted = '@BusinessObject.DirectRetroBo.RetroStatusCompleted';

    // Url
    var GetTreatyCodeByCedantUrl = '@Url.Action("Index", "TreatyCode")';
    var GetSoaDataBatchUrl = '@Url.Action("GetSoaDataBatch")';
    var DownloadStatusFileUrl = '@Url.Action("StatusFileDownload", "DirectRetro")?directRetroId=@Model.Id' + "&statusHistoryId=";
    var RetroStatementUrl = '@Url.Action("RetroStatement", "DirectRetro")?directRetroId=@Model.Id' + "&id=";

    // Listing
    var SelectedCedantId = '@Model.CedantId';
    var SelectedTreatyCodeId = '@Model.TreatyCodeId';
    var SelectedSoaDataBatchId = '@Model.SoaDataBatchId';
    var QuarterFormat = '@BusinessObject.Mfrs17ReportingBo.DateQuarterFormatJs';
    var RetroSummaries = @Html.Raw(Json.Encode(ViewBag.RetroSummaryBos));
    var TotalRetroSummary = @Html.Raw(Json.Encode(ViewBag.TotalRetroSummaryBo));
    var StatusHistories = @Html.Raw(Json.Encode(ViewBag.StatusHistories));
    var StatusFiles = @Html.Raw(Json.Encode(ViewBag.StatusFiles));
    var RetroStatements = @Html.Raw(Json.Encode(ViewBag.RetroStatements));

    // Others
    var Disabled = "@disabled";
    var EnableDownloadData = "@enableDownloadData";
</script>
