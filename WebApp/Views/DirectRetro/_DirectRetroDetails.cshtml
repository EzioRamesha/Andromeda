    <div class="tab-pane fade @(ViewBag.ActiveTab == 4 ? "show active" : "")" id="retroStatement" role="tabpanel" aria-labelledby="retroStatement-tab">
        <div class="text-right my-4">
            <div class="btn-group">
                <button type="button" class="btn btn-download dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    Template
                </button>
                <div class="dropdown-menu">
                    @Html.ActionLink("Default", "DownloadTemplate", new { type = 1 }, new { @class = "dropdown-item" })
                    @Html.ActionLink("Swiss Re", "DownloadTemplate", new { type = 2 }, new { @class = "dropdown-item" })
                </div>
            </div>
            @if (Model.Id != 0 && disabled == false && enableAddRetroStatement == true)
            {
                <a href="@Url.Action("RetroStatement", new { id = 0, directRetroId = Model.Id })" class="btn btn-add-sub">ADD NEW</a>
            }
            else
            {
                <span class="btn btn-submit isDisabled">ADD NEW</span>
            }
        </div>

        <div class="table-responsive pb-2">
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th class="w-20">Retro Party</th>
                        <th class="w-15">Accounting Period 1</th>
                        <th class="w-15">Accounting Period 2</th>
                        <th class="w-15">Accounting Period 3</th>
                        <th class="w-30">Status</th>
                        <th class="w-5">Action</th>
                    </tr>
                </thead>
                <tbody>
                    <tr v-if="!RetroStatements.length">
                        <td colspan="6" class="no-data">No available data found.</td>
                    </tr>
                    <tr v-for="(RetroStatement, index) in RetroStatements">
                        <td>{{RetroStatement.RetroPartyBo?.Party}}</td>
                        <td>{{RetroStatement.AccountingPeriod}}</td>
                        <td>{{RetroStatement.AccountingPeriod2}}</td>
                        <td>{{RetroStatement.AccountingPeriod3}}</td>
                        <td class="va-middle"><span class="status-badge">{{ RetroStatement.StatusName }}</span></td>
