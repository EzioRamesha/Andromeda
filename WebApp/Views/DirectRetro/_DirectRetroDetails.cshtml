@using PagedList.Mvc

@model WebApp.Models.DirectRetroViewModel

@{
    var disabled = ViewBag.Disabled || (ViewBag.ReadOnly == null ? false : ViewBag.ReadOnly);

    PagedList.IPagedList<WebApp.Models.RetroRiDataListingViewModel> RiDataList = ViewBag.RiDataList as PagedList.IPagedList<WebApp.Models.RetroRiDataListingViewModel>;
    PagedList.IPagedList<WebApp.Models.RetroClaimRegisterListingViewModel> ClaimRegisterList = ViewBag.ClaimRegisterList as PagedList.IPagedList<WebApp.Models.RetroClaimRegisterListingViewModel>;
    var enableAddRetroStatement = ViewBag.EnableAddRetroStatement != null ? ViewBag.EnableAddRetroStatement : false;
    var enableDownloadData = ViewBag.EnableDownloadData != null ? ViewBag.EnableDownloadData : false;
}

<ul class="nav nav-tabs">
    <li class="nav-item"><a class="@(ViewBag.ActiveTab == null || ViewBag.ActiveTab == 1 ? "active" : "") underline-tab" data-toggle="tab" href="#riData">RI Data</a></li>
    <li class="nav-item"><a class="@(ViewBag.ActiveTab == 2 ? "active" : "") underline-tab" data-toggle="tab" href="#claims">Claims</a></li>
    <li class="nav-item"><a class="@(ViewBag.ActiveTab == 3 ? "active" : "") underline-tab" data-toggle="tab" href="#retroSummary">Retro Summary</a></li>
    <li class="nav-item"><a class="@(ViewBag.ActiveTab == 4 ? "active" : "") underline-tab" data-toggle="tab" href="#retroStatement">Retro Statement</a></li>
    <li class="nav-item"><a class="@(ViewBag.ActiveTab == 5 ? "active" : "") underline-tab" data-toggle="tab" href="#statusHistory">Status History</a></li>
</ul>

<div class="tab-content underline-tab-content">
    <div class="tab-pane fade @(ViewBag.ActiveTab == null || ViewBag.ActiveTab == 1 ? "show active" : "")" id="riData" role="tabpanel" aria-labelledby="riData-tab">
        <div class="text-right my-4">
            <button type="submit" class="btn btn-download" style="width: auto;" formaction="@(Url.Action("DownloadRiData"))" :disabled="Disabled == 'True' || EnableDownloadData == 'False'">Download</button>
        </div>

        <div class="table-responsive pb-2">
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th class="w-10">Policy Number</th>
                        <th class="w-10">Treaty Code</th>
                        <th class="w-10">Transaction Type Code</th>
                        <th class="w-10">Reinsurance Basis Code</th>
                        <th class="w-10">Report Period Month</th>
                        <th class="w-10">Report Period Year</th>
                        <th class="w-10">Risk Period Month</th>
                        <th class="w-10">Risk Period Year</th>
                        <th class="w-10">Insured Name</th>

                        <th class="w-10">Retro Party</th>
                        <th class="w-10">Share (%)</th>
                        <th class="w-10">Direct Retro AAR</th>
                        <th class="w-10">Direct Retro Reinsurance Premium</th>
                        <th class="w-10">Direct Retro Discount</th>
                        <th class="w-10">Direct Retro Net Premium</th>

                        <th class="w-10">Retro Party</th>
                        <th class="w-10">Share (%)</th>
                        <th class="w-10">Direct Retro AAR</th>
                        <th class="w-10">Direct Retro Reinsurance Premium</th>
                        <th class="w-10">Direct Retro Discount</th>
                        <th class="w-10">Direct Retro Net Premium</th>

                        <th class="w-10">Retro Party</th>
                        <th class="w-10">Share (%)</th>
                        <th class="w-10">Direct Retro AAR</th>
                        <th class="w-10">Direct Retro Reinsurance Premium</th>
                        <th class="w-10">Direct Retro Discount</th>
                        <th class="w-10">Direct Retro Net Premium</th>
                    </tr>
                </thead>
                <tbody>
                    @if (RiDataList != null && RiDataList.Count > 0)
                    {
                        foreach (var item in RiDataList)
                        {
                            <tr>
                                <td>
                                    @item.PolicyNumber
                                </td>
                                <td>
                                    @item.TreatyCode
                                </td>
                                <td>
                                    @item.TransactionTypeCode
                                </td>
                                <td>
                                    @item.ReinsBasisCode
                                </td>
                                <td>
                                    @item.ReportPeriodMonth
                                </td>
                                <td>
                                    @item.ReportPeriodYear
                                </td>
                                <td>
                                    @item.RiskPeriodMonth
                                </td>
                                <td>
                                    @item.RiskPeriodYear
                                </td>
                                <td>
                                    @item.InsuredName
                                </td>
                                <td>
                                    @item.RetroParty1
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroShare1)
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroAar1)
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroReinsurancePremium1)
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroDiscount1)
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroNetPremium1)
                                </td>
                                <td>
                                    @item.RetroParty2
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroShare2)
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroAar2)
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroReinsurancePremium2)
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroDiscount2)
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroNetPremium2)
                                </td>
                                <td>
                                    @item.RetroParty3
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroShare3)
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroAar3)
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroReinsurancePremium3)
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroDiscount3)
                                </td>
                                <td class="text-right">
                                    @Shared.Util.DoubleToString(item.RetroNetPremium3)
                                </td>
                            </tr>
                        }
                    }
                    else
                    {
                        <tr>
                            <td colspan="27" class="no-data">No available data found.</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>

        @if (RiDataList != null)
        {
            {
                ViewBag.CurrentPageNumber = RiDataList.PageNumber;
                ViewBag.CurrentPageItems = RiDataList.Count;
                ViewBag.PageSize = RiDataList.PageSize;
                ViewBag.Total = ViewBag.RiDataTotal;
            }
            <div class="pagination-footer position-relative">
                <div class="row">
                    <div class="col-md-4">
                        @Html.Partial("_PageInfo")
                    </div>
                    <div class="col-md-8">
                        @Html.PagedListPager(RiDataList, Page => Url.Action("Edit", new
                       {
                           RiDataPage = Page,
                           ClaimRegisterPage = ViewBag.ClaimRegisterPage,
                           TabIndex = 1
                       }), Shared.Html.GetPagedListRenderOptions())
                    </div>
                </div>
            </div>
        }
    </div>

    <div class="tab-pane fade @(ViewBag.ActiveTab == 2 ? "show active" : "")" id="claims" role="tabpanel" aria-labelledby="claims-tab">
        <div class="text-right my-4">
