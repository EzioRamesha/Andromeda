    <DataSet Name="ClaimRegisterTable">
      <Query>
        <DataSourceName>ClaimsDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromMonth">
            <Value>=Parameters!FromMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToMonth">
            <Value>=Parameters!ToMonth.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@FromYear">
            <Value>=Parameters!FromYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToYear">
            <Value>=Parameters!ToYear.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Cedant">
            <Value>=Parameters!Cedant.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RecordType">
            <Value>=Parameters!RecordType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 1 AS RowType, 'Details' AS RowLabel
, @ToYear AS Value1
, CASE WHEN CAST(@ToYear AS INT) - CAST(@FromYear AS INT) &gt;= 1 THEN CAST(@ToYear AS INT) - 1 ELSE 0 END AS Value2
, CASE WHEN CAST(@ToYear AS INT) - CAST(@FromYear AS INT) &gt;= 2 THEN CAST(@ToYear AS INT) - 2 ELSE 0 END AS Value3
, CASE WHEN CAST(@ToYear AS INT) - CAST(@FromYear AS INT) &gt;= 3 THEN CAST(@ToYear AS INT) - 3 ELSE 0 END AS Value4
, CASE WHEN CAST(@ToYear AS INT) - CAST(@FromYear AS INT) &gt;= 4 THEN CAST(@ToYear AS INT) - 4 ELSE 0 END AS Value5
-----
UNION
-----
SELECT 2 AS RowType, 'Claim Amount' AS RowLabel
, ISNULL((SELECT SUM(t.ClaimRecoveryAmt) AS Value2 FROM ClaimRegister t
WHERE (MONTH(DateOfRegister) BETWEEN @FromMonth AND @ToMonth) AND (YEAR(DateOfRegister) = @ToYear)
AND t.RecordType = @RecordType AND t.CedingCompany = @Cedant
GROUP BY t.CedingCompany, t.RecordType),0) AS Value1

, CASE WHEN CAST(@ToYear AS INT) - CAST(@FromYear AS INT) &gt;= 1 THEN ISNULL((SELECT SUM(t.ClaimRecoveryAmt) AS Value2 FROM ClaimRegister t
WHERE (MONTH(DateOfRegister) BETWEEN @FromMonth AND @ToMonth) AND (YEAR(DateOfRegister) = CAST(@ToYear AS INT) - 1)
AND t.RecordType = @RecordType AND t.CedingCompany = @Cedant
GROUP BY t.CedingCompany, t.RecordType),0) ELSE 0 END AS Value2

, CASE WHEN CAST(@ToYear AS INT) - CAST(@FromYear AS INT) &gt;= 2 THEN ISNULL((SELECT SUM(t.ClaimRecoveryAmt) AS Value2 FROM ClaimRegister t
WHERE (MONTH(DateOfRegister) BETWEEN @FromMonth AND @ToMonth) AND (YEAR(DateOfRegister) = CAST(@ToYear AS INT) - 2)
AND t.RecordType = @RecordType AND t.CedingCompany = @Cedant
GROUP BY t.CedingCompany, t.RecordType),0) ELSE 0 END AS Value3

, CASE WHEN CAST(@ToYear AS INT) - CAST(@FromYear AS INT) &gt;= 3 THEN ISNULL((SELECT SUM(t.ClaimRecoveryAmt) AS Value2 FROM ClaimRegister t
WHERE (MONTH(DateOfRegister) BETWEEN @FromMonth AND @ToMonth) AND (YEAR(DateOfRegister) = CAST(@ToYear AS INT) - 3)
AND t.RecordType = @RecordType AND t.CedingCompany = @Cedant
GROUP BY t.CedingCompany, t.RecordType),0) ELSE 0 END AS Value4

, CASE WHEN CAST(@ToYear AS INT) - CAST(@FromYear AS INT) &gt;= 4 THEN ISNULL((SELECT SUM(t.ClaimRecoveryAmt) AS Value2 FROM ClaimRegister t
WHERE (MONTH(DateOfRegister) BETWEEN @FromMonth AND @ToMonth) AND (YEAR(DateOfRegister) = CAST(@ToYear AS INT) - 4)
AND t.RecordType = @RecordType AND t.CedingCompany = @Cedant
GROUP BY t.CedingCompany, t.RecordType),0) ELSE 0 END AS Value5
-----
UNION
-----
SELECT 3 AS RowType, 'Claim Count' AS RowLabel
, ISNULL((SELECT COUNT(t.ClaimRecoveryAmt) AS Value2 FROM ClaimRegister t
WHERE (MONTH(DateOfRegister) BETWEEN @FromMonth AND @ToMonth) AND (YEAR(DateOfRegister) = @ToYear)
AND t.RecordType = @RecordType AND t.CedingCompany = @Cedant
GROUP BY t.CedingCompany, t.RecordType),0) AS Value1

, CASE WHEN CAST(@ToYear AS INT) - CAST(@FromYear AS INT) &gt;= 1 THEN ISNULL((SELECT COUNT(t.ClaimRecoveryAmt) AS Value2 FROM ClaimRegister t
WHERE (MONTH(DateOfRegister) BETWEEN @FromMonth AND @ToMonth) AND (YEAR(DateOfRegister) = CAST(@ToYear AS INT) - 1)
AND t.RecordType = @RecordType AND t.CedingCompany = @Cedant
GROUP BY t.CedingCompany, t.RecordType),0) ELSE 0 END AS Value2

, CASE WHEN CAST(@ToYear AS INT) - CAST(@FromYear AS INT) &gt;= 2 THEN ISNULL((SELECT COUNT(t.ClaimRecoveryAmt) AS Value2 FROM ClaimRegister t
WHERE (MONTH(DateOfRegister) BETWEEN @FromMonth AND @ToMonth) AND (YEAR(DateOfRegister) = CAST(@ToYear AS INT) - 2)
AND t.RecordType = @RecordType AND t.CedingCompany = @Cedant
GROUP BY t.CedingCompany, t.RecordType),0) ELSE 0 END AS Value3

, CASE WHEN CAST(@ToYear AS INT) - CAST(@FromYear AS INT) &gt;= 3 THEN ISNULL((SELECT COUNT(t.ClaimRecoveryAmt) AS Value2 FROM ClaimRegister t
WHERE (MONTH(DateOfRegister) BETWEEN @FromMonth AND @ToMonth) AND (YEAR(DateOfRegister) = CAST(@ToYear AS INT) - 3)
AND t.RecordType = @RecordType AND t.CedingCompany = @Cedant
GROUP BY t.CedingCompany, t.RecordType),0) ELSE 0 END AS Value4

, CASE WHEN CAST(@ToYear AS INT) - CAST(@FromYear AS INT) &gt;= 4 THEN ISNULL((SELECT COUNT(t.ClaimRecoveryAmt) AS Value2 FROM ClaimRegister t
WHERE (MONTH(DateOfRegister) BETWEEN @FromMonth AND @ToMonth) AND (YEAR(DateOfRegister) = CAST(@ToYear AS INT) - 4)
AND t.RecordType = @RecordType AND t.CedingCompany = @Cedant
GROUP BY t.CedingCompany, t.RecordType),0) ELSE 0 END AS Value5</CommandText>
      </Query>
