  <DataSets>
    <DataSet Name="ClaimRegister">
      <Query>
        <DataSourceName>ClaimsDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromMonth">
            <Value>=Parameters!FromMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToMonth">
            <Value>=Parameters!ToMonth.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@FromYear">
            <Value>=Parameters!FromYear.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@ToYear">
            <Value>=Parameters!ToYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Cedant">
            <Value>=Parameters!Cedant.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RecordType">
            <Value>=Parameters!RecordType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	t.CedingCompany
	, t.RecordType
	, t.ReportedMonth
	, t.ReportedYear
	, COUNT(t.ClaimRecoveryAmt) AS RowValue
	, 'Count' AS RowType
FROM (SELECT
		c.CedingCompany
		, MONTH(c.DateOfRegister) AS ReportedMonth
		, YEAR(c.DateOfRegister) AS ReportedYear
		, c.RecordType
		, c.ClaimRecoveryAmt
	FROM ClaimRegister c
	WHERE c.DateOfRegister IS NOT NULL) t
WHERE t.ReportedMonth BETWEEN @FromMonth AND @ToMonth
AND t.ReportedYear BETWEEN (CASE WHEN (CAST(@ToYear AS INT) - CAST(@FromYear AS INT) &gt; 4) THEN CAST(@ToYear AS INT) - 4 ELSE @FromYear END) AND @ToYear
AND t.CedingCompany = @Cedant
AND t.RecordType = @RecordType
GROUP BY t.CedingCompany, t.RecordType, t.ReportedMonth, t.ReportedYear

UNION

SELECT
	t.CedingCompany
	, t.RecordType
	, t.ReportedMonth
	, t.ReportedYear
	, SUM(t.ClaimRecoveryAmt) / 1000000 AS RowValue
	, 'Sum RI' AS RowType
FROM (SELECT
		c.CedingCompany
		, MONTH(c.DateOfRegister) AS ReportedMonth
		, YEAR(c.DateOfRegister) AS ReportedYear
		, c.RecordType
		, c.ClaimRecoveryAmt
	FROM ClaimRegister c
	WHERE c.DateOfRegister IS NOT NULL) t
WHERE t.ReportedMonth BETWEEN @FromMonth AND @ToMonth
AND t.ReportedYear BETWEEN (CASE WHEN (CAST(@ToYear AS INT) - CAST(@FromYear AS INT) &gt; 4) THEN CAST(@ToYear AS INT) - 4 ELSE @FromYear END) AND @ToYear
AND t.CedingCompany = @Cedant
AND t.RecordType = @RecordType
GROUP BY t.CedingCompany, t.RecordType, t.ReportedMonth, t.ReportedYear</CommandText>
      </Query>
      <Fields>
        <Field Name="CedingCompany">
          <DataField>CedingCompany</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RecordType">
          <DataField>RecordType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ReportedMonth">
          <DataField>ReportedMonth</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ReportedYear">
          <DataField>ReportedYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="RowValue">
          <DataField>RowValue</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="RowType">
          <DataField>RowType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
