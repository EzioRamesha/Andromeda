  <DataSets>
    <DataSet Name="ClaimRegister">
      <Query>
        <DataSourceName>ClaimsAbove1MilDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromMonth">
            <Value>=Parameters!FromMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FromYear">
            <Value>=Parameters!FromYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToMonth">
            <Value>=Parameters!ToMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToYear">
            <Value>=Parameters!ToYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Cedant">
            <Value>=Parameters!Cedant.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	c.CedingCompany
	, c.InsuredName
	, c.ClaimCode
	, SUM(c.ClaimRecoveryAmt) AS SumReinsured

	, (SELECT COUNT(t.ClaimRecoveryAmt) FROM ClaimRegister t WHERE t.CedingCompany = c.CedingCompany AND t.InsuredName = c.InsuredName
	AND t.ClaimCode = c.ClaimCode AND t.ClaimRecoveryAmt &gt; 1000000 AND t.DateOfRegister IS NOT NULL AND (t.DateOfRegister BETWEEN
	DATEFROMPARTS(@FromYear, @FromMonth, 01) AND EOMONTH(DATEFROMPARTS(@ToYear, @ToMonth, 01)))) AS NoOfClaims

FROM ClaimRegister c
WHERE c.DateOfRegister IS NOT NULL
AND (c.DateOfRegister BETWEEN DATEFROMPARTS(@FromYear, @FromMonth, 01) AND EOMONTH(DATEFROMPARTS(@ToYear, @ToMonth, 01)))
AND c.CedingCompany IN (@Cedant)
AND c.ClaimRecoveryAmt &gt; 1000000
GROUP BY c.CedingCompany, c.InsuredName, c.ClaimCode</CommandText>
      </Query>
      <Fields>
        <Field Name="CedingCompany">
          <DataField>CedingCompany</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="InsuredName">
          <DataField>InsuredName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ClaimCode">
          <DataField>ClaimCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SumReinsured">
          <DataField>SumReinsured</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="NoOfClaims">
          <DataField>NoOfClaims</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
