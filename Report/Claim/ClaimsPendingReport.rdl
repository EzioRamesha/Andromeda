  <DataSets>
    <DataSet Name="ClaimRegister">
      <Query>
        <DataSourceName>ClaimsPendingDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromMonth">
            <Value>=Parameters!FromMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FromYear">
            <Value>=Parameters!FromYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToMonth">
            <Value>=Parameters!ToMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToYear">
            <Value>=Parameters!ToYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ClaimStatus">
            <Value>=Parameters!ClaimStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ClaimCode">
            <Value>=Parameters!ClaimCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RecordType">
            <Value>=Parameters!RecordType.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@ClaimReason">
            <Value>=Parameters!ClaimReason.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	PicClaimId
	, ClaimId
	, CedingCompany
	, ISNULL(ClaimReasonId,0) AS ClaimReasonId
	, CASE WHEN DATEDIFF(MONTH, DateOfRegister, GETDATE()) &lt; 4 THEN 'within 3 mths'
		WHEN DATEDIFF(MONTH, DateOfRegister, GETDATE()) BETWEEN 4 AND 6 THEN '4 - 6 mths'
		WHEN DATEDIFF(MONTH, DateOfRegister, GETDATE()) BETWEEN 7 AND 9 THEN '7 - 9 mths'
		WHEN DATEDIFF(MONTH, DateOfRegister, GETDATE()) BETWEEN 10 AND 12 THEN '10 - 12 mths'
		WHEN DATEDIFF(MONTH, DateOfRegister, GETDATE()) BETWEEN 13 AND 24 THEN '13 - 24 mths'
		WHEN DATEDIFF(MONTH, DateOfRegister, GETDATE()) &gt; 24 THEN 'above 24 mths'
		END AS 'AgeingRange'
	, ClaimRecoveryAmt
	, DateOfRegister
	, ClaimStatus
	, ISNULL(ClaimCode,'') AS ClaimCode
	, ISNULL(RecordType,'') AS RecordType
FROM ClaimRegister
WHERE DateOfRegister IS NOT NULL
AND (DateOfRegister BETWEEN DATEFROMPARTS(@FromYear, @FromMonth, 01) AND EOMONTH(DATEFROMPARTS(@ToYear, @ToMonth, 01)))
AND ClaimStatus IN (@ClaimStatus)
AND ClaimCode IN (@ClaimCode)
AND RecordType IN (@RecordType)
AND ClaimReasonId IN (@ClaimReason) OR ClaimReasonId IS NULL</CommandText>
      </Query>
      <Fields>
