      <DataSourceReference>/Andromeda2aDataSource</DataSourceReference>
      <rd:SecurityType>DataBase</rd:SecurityType>
      <rd:DataSourceID>2148c69e-a194-4ea9-8f49-8f8b5b6c5085</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="TreatyPricingQuotationWorkflows">
      <Query>
        <DataSourceName>QuotationReportDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @Table TABLE(
	CedingCompany NVARCHAR(255),
	ReinsuranceType NVARCHAR(255),
	QuotationId NVARCHAR(255),
	LatestVersion NVARCHAR(255),
	Status INT,
	QuotationName NVARCHAR(255),
	ExpectedRIPremium NVARCHAR(255),
	StatusLastUpdated DATETIME,
	BusinessType NVARCHAR(255)
)

INSERT INTO @Table
SELECT * FROM
	(SELECT c.Code + ' - ' + c.Name AS CedingCompany
		, p1.Description AS ReinsuranceType
		, w.QuotationId
		, CAST(w.LatestVersion AS NVARCHAR(30)) + '.0' AS LatestVersion
		, ISNULL(s.Status, 1) AS Status
		, w.Name AS QuotationName
		, ISNULL((SELECT TOP 1 pv.ExpectedRiPremium FROM TreatyPricingWorkflowObjects o
			LEFT JOIN TreatyPricingProductVersions pv ON pv.Id = o.ObjectVersionId
			WHERE o.Type = 1 AND o.ObjectType = 8 AND o.WorkflowId = w.Id), 0) AS ExpectedRIPremium
		, ISNULL(s.CreatedAt, w.CreatedAt) AS StatusLastUpdated
		, p2.Description AS BusinessType
	FROM TreatyPricingQuotationWorkflows w
	LEFT JOIN StatusHistories s ON w.Id = s.ObjectId AND s.ModuleId = 145 AND s.SubModuleController IS NULL
	LEFT JOIN Cedants c ON c.Id = w.CedantId
	LEFT JOIN TreatyPricingWorkflowObjects o ON o.WorkflowId = w.Id AND o.ObjectType = 8 AND o.[Type] = 1
	LEFT JOIN TreatyPricingProductVersions pv ON pv.Id = o.ObjectVersionId
	LEFT JOIN PickListDetails p1 ON p1.Id = w.ReinsuranceTypePickListDetailId
	LEFT JOIN PickListDetails p2 ON p2.Id = pv.BusinessTypePickListDetailId) t

DECLARE @QuotationId NVARCHAR(255)
DECLARE QuotationIdCursor CURSOR FOR 
SELECT QuotationId FROM @Table WHERE Status &lt;&gt; 1

OPEN QuotationIdCursor

FETCH NEXT FROM QuotationIdCursor INTO @QuotationId

WHILE @@FETCH_STATUS = 0
BEGIN
	INSERT INTO @Table
	SELECT TOP 1 c.Code + ' - ' + c.Name AS CedingCompany
		, p1.Description AS ReinsuranceType
		, w.QuotationId
		, CAST(w.LatestVersion AS NVARCHAR(30)) + '.0' AS LatestVersion
		, 1 AS Status
		, w.Name AS QuotationName
		, ISNULL((SELECT TOP 1 pv.ExpectedRiPremium FROM TreatyPricingWorkflowObjects o
			LEFT JOIN TreatyPricingProductVersions pv ON pv.Id = o.ObjectVersionId
			WHERE o.Type = 1 AND o.ObjectType = 8 AND o.WorkflowId = w.Id), 0) AS ExpectedRIPremium
		, w.CreatedAt AS StatusLastUpdated
		, p2.Description AS BusinessType
	FROM TreatyPricingQuotationWorkflows w
	LEFT JOIN Cedants c ON c.Id = w.CedantId
	LEFT JOIN TreatyPricingWorkflowObjects o ON o.WorkflowId = w.Id AND o.ObjectType = 8 AND o.[Type] = 1
	LEFT JOIN TreatyPricingProductVersions pv ON pv.Id = o.ObjectVersionId
	LEFT JOIN PickListDetails p1 ON p1.Id = w.ReinsuranceTypePickListDetailId
	LEFT JOIN PickListDetails p2 ON p2.Id = pv.BusinessTypePickListDetailId
	WHERE w.QuotationId = @QuotationId

	FETCH NEXT FROM QuotationIdCursor INTO @QuotationId
END

CLOSE QuotationIdCursor
DEALLOCATE QuotationIdCursor

SELECT * FROM @Table
WHERE StatusLastUpdated BETWEEN @FromDate AND DATEADD(SECOND, -1, DATEADD(DAY, 1, @ToDate))
--OR Status = 1 AND StatusLastUpdated &lt;= @FromDate</CommandText>
      </Query>
