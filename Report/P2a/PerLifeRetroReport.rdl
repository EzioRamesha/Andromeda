    <DataSet Name="Cedant">
      <Query>
        <DataSourceName>PerLifeRetroReportDataSource</DataSourceName>
        <CommandText>SELECT
	Id AS CedantId,
	Code + ' - ' + Name AS CedantDisplay
FROM Cedants
ORDER BY Code, Name</CommandText>
      </Query>
