      <DataSourceReference>/Andromeda2aDataSource</DataSourceReference>
      <rd:SecurityType>DataBase</rd:SecurityType>
      <rd:DataSourceID>2148c69e-a194-4ea9-8f49-8f8b5b6c5085</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="TreatyPricingPerLifeRetro">
      <Query>
        <DataSourceName>PerLifeRetroReportDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Cedant">
            <Value>=Parameters!Cedant.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RetroShare">
            <Value>=Parameters!RetroShare.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @Output TABLE(
	Id INT, ProductID NVARCHAR(100), ProductName NVARCHAR(100), CedantId INT, CedingCompany NVARCHAR(100), PerLifeRetro NVARCHAR(10),
	TreatyCode1 NVARCHAR(50), TreatyCode2 NVARCHAR(50), TreatyCode3 NVARCHAR(50), RetroShare1 FLOAT, RetroShare2 FLOAT, RetroShare3 FLOAT,
	TotalShare FLOAT)

DECLARE @Id INT, @HasPerLifeRetro BIT, @RetroCount INT, @RetroID INT
DECLARE productsCursor CURSOR FOR
SELECT DISTINCT p.Id, p.HasPerLifeRetro, (SELECT COUNT(*) FROM TreatyPricingPerLifeRetroProducts WHERE TreatyPricingProductId = p.Id)
FROM TreatyPricingProducts p

OPEN productsCursor
FETCH NEXT FROM productsCursor INTO @Id, @HasPerLifeRetro, @RetroCount

WHILE @@FETCH_STATUS = 0
BEGIN
	INSERT INTO @Output (Id, ProductID, ProductName, CedantId, CedingCompany, PerLifeRetro,
	TreatyCode1, TreatyCode2, TreatyCode3, RetroShare1, RetroShare2, RetroShare3)
	(SELECT p.Id, p.Code, p.[Name], c.CedantId, ce.Code + ' - ' + ce.[Name] AS CedingCompany
	, CASE WHEN ISNULL(HasPerLifeRetro,0) = 1 THEN 'Y' ELSE 'N' END
	, '', '', '', 0, 0, 0
	FROM TreatyPricingProducts p
	LEFT JOIN TreatyPricingCedants c ON c.Id = p.TreatyPricingCedantId
	LEFT JOIN Cedants ce ON ce.Id = c.CedantId
	WHERE p.Id = @Id)

	IF (ISNULL(@HasPerLifeRetro,0) = 1)
	BEGIN
		IF (@RetroCount &gt;= 1)
		BEGIN
			SET @RetroID = (SELECT TOP 1 t.TreatyPricingPerLifeRetroId FROM
				(SELECT TOP 3 * FROM TreatyPricingPerLifeRetroProducts rp
				WHERE rp.TreatyPricingProductId = @Id
				ORDER BY rp.TreatyPricingPerLifeRetroId DESC) t
				ORDER BY t.TreatyPricingPerLifeRetroId)

			UPDATE @Output SET
				TreatyCode1 = (SELECT TOP 1 Code FROM TreatyPricingPerLifeRetro WHERE Id = @RetroID)
				, RetroShare1 = (SELECT TOP 1 RetrocessionaireShare FROM TreatyPricingPerLifeRetro WHERE Id = @RetroID)
			WHERE Id = @Id
		END

		IF (@RetroCount &gt;= 2)
		BEGIN
			SET @RetroID = (SELECT TOP 1 t.TreatyPricingPerLifeRetroId FROM
				(SELECT TOP 2 *
				FROM TreatyPricingPerLifeRetroProducts rp
				WHERE rp.TreatyPricingProductId = @Id
				ORDER BY rp.TreatyPricingPerLifeRetroId) t
				ORDER BY t.TreatyPricingPerLifeRetroId DESC)

			UPDATE @Output SET
				TreatyCode2 = (SELECT TOP 1 Code FROM TreatyPricingPerLifeRetro WHERE Id = @RetroID)
				, RetroShare2 = (SELECT TOP 1 RetrocessionaireShare FROM TreatyPricingPerLifeRetro WHERE Id = @RetroID)
			WHERE Id = @Id
		END
		
		IF (@RetroCount &gt;= 3)
		BEGIN
			SET @RetroID = (SELECT TOP 1 t.TreatyPricingPerLifeRetroId FROM
				(SELECT TOP 3 *
				FROM TreatyPricingPerLifeRetroProducts rp
				WHERE rp.TreatyPricingProductId = @Id
				ORDER BY rp.TreatyPricingPerLifeRetroId DESC) t
				ORDER BY t.TreatyPricingPerLifeRetroId DESC)

			UPDATE @Output SET
				TreatyCode3 = (SELECT TOP 1 Code FROM TreatyPricingPerLifeRetro WHERE Id = @RetroID)
				, RetroShare3 = (SELECT TOP 1 RetrocessionaireShare FROM TreatyPricingPerLifeRetro WHERE Id = @RetroID)
			WHERE Id = @Id
		END
	END

	UPDATE @Output SET TotalShare = RetroShare1 + RetroShare2 + RetroShare3
	WHERE Id = @Id

	FETCH NEXT FROM productsCursor INTO @Id, @HasPerLifeRetro, @RetroCount
END

CLOSE productsCursor
DEALLOCATE productsCursor

SELECT * FROM @Output
WHERE CedantId IN (@Cedant)
AND (('1' IN (@RetroShare) AND TotalShare &lt; 100) OR
('2' IN (@RetroShare) AND TotalShare = 100) OR
('3' IN (@RetroShare) AND TotalShare &gt; 100))
ORDER BY ProductID</CommandText>
      </Query>
