    <DataSet Name="RetroShare">
      <Query>
        <DataSourceName>PerLifeRetroReportDataSource</DataSourceName>
        <CommandText>SELECT 1 AS RetroShare, '&lt; 100%' AS Description
UNION
SELECT 2 AS RetroShare, '100%' AS Description
UNION
SELECT 3 AS RetroShare, '&gt; 100%' AS Description</CommandText>
