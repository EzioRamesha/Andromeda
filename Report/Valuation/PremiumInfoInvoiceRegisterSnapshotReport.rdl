    <DataSet Name="InvoiceRegisterHistory">
      <Query>
        <DataSourceName>InvoiceRegisterDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@InvoiceStartDate">
            <Value>=Format(Parameters!InvoiceStartDate.Value,"MM/dd/yyyy")</Value>
          </QueryParameter>
          <QueryParameter Name="@InvoiceEndDate">
            <Value>=Format(Parameters!InvoiceEndDate.Value,"MM/dd/yyyy")</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportingQuarter">
            <Value>=Parameters!ReportingQuarter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CedantId">
            <Value>=Parameters!CedantId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TreatyCodeId">
            <Value>=Parameters!TreatyCodeId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TreatyTypeId">
            <Value>=Parameters!TreatyTypeId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SoaQuarter">
            <Value>=Parameters!SoaQuarter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RiskQuarter">
            <Value>=Parameters!RiskQuarter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RiskQuarterYear">
            <Value>=Parameters!RiskQuarterYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SoaQuarterYear">
            <Value>=Parameters!SoaQuarterYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@InvoiceType">
            <Value>=Parameters!InvoiceType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Frequency">
            <Value>=Parameters!Frequency.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  InvoiceRegisterHistories.CutOffId
  ,InvoiceRegisterHistories.RiskQuarter
  ,InvoiceRegisterHistories.InvoiceDate
  ,CONCAT(RIGHT(DATEPART(YEAR, InvoiceRegisterHistories.InvoiceDate),2), 'Q', DATEPART(QUARTER, InvoiceRegisterHistories.InvoiceDate)) AS InvoiceQuarter
  ,SUM(ISNULL(InvoiceRegisterHistories.ValuationGross1st, 0)) AS ValuationGross1st
  ,SUM(ISNULL(InvoiceRegisterHistories.ValuationGrossRen, 0)) AS ValuationGrossRen
  ,SUM(ISNULL(InvoiceRegisterHistories.ValuationDiscount1st, 0)) AS ValuationDiscount1st
  ,SUM(ISNULL(InvoiceRegisterHistories.ValuationDiscountRen, 0)) AS ValuationDiscountRen
  ,SUM(ISNULL(InvoiceRegisterHistories.ValuationCom1st, 0)) AS ValuationCom1st
  ,SUM(ISNULL(InvoiceRegisterHistories.ValuationComRen, 0)) AS ValuationComRen
  ,SUM(ISNULL(InvoiceRegisterHistories.ValuationRiskPremium, 0)) AS RiskPremium
  ,SUM(ISNULL(InvoiceRegisterHistories.ValuationProfitCom, 0)) AS ProfitComm
  ,SUM(ISNULL(InvoiceRegisterHistories.ValuationClaims, 0)) AS Claim
  ,SUM(ISNULL(InvoiceRegisterHistories.SurrenderValue, 0)) AS SurrenderValue
  ,SUM(ISNULL(InvoiceRegisterHistories.DTH, 0)) AS DTH
  ,SUM(ISNULL(InvoiceRegisterHistories.TPD, 0)) AS TPD
  ,SUM(ISNULL(InvoiceRegisterHistories.CI, 0)) AS CI
  ,SUM(ISNULL(InvoiceRegisterHistories.PA, 0)) AS PA
  ,SUM(ISNULL(InvoiceRegisterHistories.HS, 0)) AS HS
  ,TreatyCodes.Code AS TreatyCode
  ,TreatyCodes.TreatyTypePickListDetailId
  ,Cedants.Name AS CedantName
FROM
  InvoiceRegisterHistories
INNER JOIN Cedants ON InvoiceRegisterHistories.CedantId = Cedants.Id
INNER JOIN TreatyCodes ON InvoiceRegisterHistories.TreatyCodeId = TreatyCodes.Id
WHERE InvoiceRegisterHistories.InvoiceDate BETWEEN @InvoiceStartDate and @InvoiceEndDate
AND InvoiceRegisterHistories.CutOffId = @ReportingQuarter
AND InvoiceRegisterHistories.CedantId = @CedantId
AND InvoiceRegisterHistories.TreatyCodeId IN (@TreatyCodeId)
AND TreatyCodes.TreatyTypePickListDetailId IN (@TreatyTypeId)
AND SUBSTRING(InvoiceRegisterHistories.RiskQuarter, 3, 2) IN (@RiskQuarter)
AND SUBSTRING(InvoiceRegisterHistories.RiskQuarter, 1, 2) IN (@RiskQuarterYear)
AND SUBSTRING(InvoiceRegisterHistories.SoaQuarter, 3, 2) IN (@SoaQuarter)
AND SUBSTRING(InvoiceRegisterHistories.SoaQuarter, 1, 2) IN (@SoaQuarterYear)
AND InvoiceRegisterHistories.InvoiceType IN (@InvoiceType)
AND ISNULL(CAST(InvoiceRegisterHistories.Frequency AS Varchar(5)), 'NULL') IN (@Frequency)
AND InvoiceRegisterHistories.ReportingType = 2
GROUP BY InvoiceRegisterHistories.RiskQuarter, TreatyCodes.Code, TreatyCodes.TreatyTypePickListDetailId, Cedants.Name, InvoiceRegisterHistories.InvoiceDate, InvoiceRegisterHistories.CutOffId
ORDER BY InvoiceRegisterHistories.RiskQuarter</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
