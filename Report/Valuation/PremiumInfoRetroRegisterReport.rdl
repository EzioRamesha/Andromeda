    <DataSet Name="SumOfLookup">
      <Query>
        <DataSourceName>RetroRegisterDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FieldType">
            <Value>=Parameters!FieldType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TypeId">
            <Value>=Parameters!TypeId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF @TypeId = 1 AND @FieldType = 1
    SELECT [Label], [Value] FROM 
	(
		SELECT 'Valuation - Gross-1st' AS [Label], 1 AS [Value], 1 AS [Index]
		UNION
		SELECT 'Valuation - Gross-Ren' AS [Label], 2 AS [Value], 2 AS [Index]
		UNION
		SELECT 'Risk Premium' AS [Label], 3 AS [Value], 3 AS [Index]
		UNION
		SELECT 'Valuation - Discount-1st' AS [Label], 4 AS [Value], 4 AS [Index]
		UNION
		SELECT 'Valuation - Discount-Ren' AS [Label], 5 AS [Value], 5 AS [Index]
		UNION
		SELECT 'Valuation - Com-1st' AS [Label], 6 AS [Value], 6 AS [Index]
		UNION
		SELECT 'Valuation - Com-Ren' AS [Label], 7 AS [Value], 7 AS [Index]
		UNION 
		SELECT 'Profit Commission' AS [Label], 8 AS [Value], 8 AS [Index]
		UNION 
		SELECT 'Claims' AS [Label], 9 AS [Value], 9 AS [Index]
                UNION 
		SELECT 'Surrender Value' AS [Label], 10 AS [Value], 7 AS [Index]	
	) d
	ORDER BY d.[Index]
ELSE IF @TypeId = 1 AND @FieldType = 2
	SELECT [Label], [Value] FROM 
	(
		SELECT 'Reinsurance Premium' AS [Label], 12 AS [Value], 1 AS [Index]
		UNION 
		SELECT 'Risk Premium' AS [Label], 3 AS [Value], 2 AS [Index]
		UNION
		SELECT 'Reinsurance Discount' AS [Label], 13 AS [Value], 3 AS [Index]
		UNION 
		SELECT 'Reinsurance Commission' AS [Label], 14 AS [Value], 4 AS [Index]
		UNION 
		SELECT 'Profit Commission' AS [Label], 8 AS [Value], 5 AS [Index]
		UNION 
		SELECT 'Claims' AS [Label], 9 AS [Value], 6 AS [Index]
		UNION 
		SELECT 'Surrender Value' AS [Label], 10 AS [Value], 7 AS [Index]
	) d
	ORDER BY d.[Index]
ELSE IF @TypeId &lt;&gt; 1 AND @FieldType = 1
    SELECT [Label], [Value] FROM 
	(
                SELECT '(All)' AS [Label], 1 AS [Value], 0 AS [Index]
                UNION
		SELECT 'Valuation - Gross-1st' AS [Label], 2 AS [Value], 1 AS [Index]
		UNION
		SELECT 'Valuation - Gross-Ren' AS [Label], 3 AS [Value], 2 AS [Index]
		UNION
		SELECT 'Risk Premium' AS [Label], 4 AS [Value], 3 AS [Index]
		UNION
		SELECT 'Valuation - Discount-1st' AS [Label], 5 AS [Value], 4 AS [Index]
		UNION
		SELECT 'Valuation - Discount-Ren' AS [Label], 6 AS [Value], 5 AS [Index]
		UNION
		SELECT 'Valuation - Com-1st' AS [Label], 7 AS [Value], 6 AS [Index]
		UNION
		SELECT 'Valuation - Com-Ren' AS [Label], 8 AS [Value], 7 AS [Index]
		UNION 
		SELECT 'Profit Commission' AS [Label], 9 AS [Value], 8 AS [Index]
		UNION 
		SELECT 'Claims' AS [Label], 10 AS [Value], 9 AS [Index]
                UNION 
		SELECT 'Surrender Value' AS [Label], 11 AS [Value], 10 AS [Index]
	) d
	ORDER BY d.[Index]
ELSE IF @TypeId &lt;&gt; 1 AND @FieldType = 2
	SELECT [Label], [Value] FROM 
	(
                SELECT '(All)' AS [Label], 12 AS [Value], 1 AS [Index]
                UNION
		SELECT 'Reinsurance Premium' AS [Label], 13 AS [Value], 1 AS [Index]
		UNION 
		SELECT 'Risk Premium' AS [Label], 4 AS [Value], 2 AS [Index]
		UNION
		SELECT 'Reinsurance Discount' AS [Label], 14 AS [Value], 3 AS [Index]
		UNION 
		SELECT 'Reinsurance Commission' AS [Label], 15 AS [Value], 4 AS [Index]
		UNION 
		SELECT 'Profit Commission' AS [Label], 9 AS [Value], 5 AS [Index]
		UNION 
		SELECT 'Claims' AS [Label], 10 AS [Value], 6 AS [Index]
		UNION 
		SELECT 'Surrender Value' AS [Label], 11 AS [Value], 7 AS [Index]
	) d
	ORDER BY d.[Index]
ELSE
	SELECT '-' AS [Label], 1 AS [Value], 1 AS [Index]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Label">
          <DataField>Label</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Value">
