    <DataSet Name="RetroRegister">
      <Query>
        <DataSourceName>RetroRegisterDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@InvoiceStartDate">
            <Value>=Format(Parameters!InvoiceStartDate.Value,"yyyy-MM-dd")</Value>
          </QueryParameter>
          <QueryParameter Name="@InvoiceEndDate">
            <Value>=Format(Parameters!InvoiceEndDate.Value,"yyyy-MM-dd HH:mm:ss")</Value>
          </QueryParameter>
          <QueryParameter Name="@CedantId">
            <Value>=Parameters!CedantId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RetroPartyId">
            <Value>=Parameters!RetroPartyId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TreatyCodeId">
            <Value>=Parameters!TreatyCodeId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TreatyType">
            <Value>=Parameters!TreatyType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RiskQuarter">
            <Value>=Parameters!RiskQuarter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SoaQuarter">
            <Value>=Parameters!SoaQuarter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@InvoiceType">
            <Value>=Parameters!InvoiceType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Frequency">
            <Value>=Parameters!Frequency.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  RetroRegister.RiskQuarter
  ,RetroRegister.RetroStatementDate
  ,SUM(ISNULL(RetroRegister.ValuationGross1st, 0)) AS ValuationGross1st
  ,SUM(ISNULL(RetroRegister.ValuationGrossRen, 0)) AS ValuationGrossRen
  ,SUM(ISNULL(RetroRegister.ValuationDiscount1st, 0)) AS ValuationDiscount1st
  ,SUM(ISNULL(RetroRegister.ValuationDiscountRen, 0)) AS ValuationDiscountRen
  ,SUM(ISNULL(RetroRegister.ValuationCom1st, 0)) AS ValuationCom1st
  ,SUM(ISNULL(RetroRegister.ValuationComRen, 0)) AS ValuationComRen
  ,SUM(ISNULL(RetroRegister.RiskPremium, 0)) AS RiskPremium
  ,SUM(ISNULL(RetroRegister.ProfitCommission, 0)) AS ProfitCommission
  ,SUM(ISNULL(RetroRegister.Claims, 0)) AS Claims
  ,SUM(ISNULL(RetroRegister.SurrenderVal, 0)) AS SurrenderVal
  ,RetroRegister.TreatyType
FROM
  RetroRegister
WHERE RetroRegister.RetroStatementDate &gt;= @InvoiceStartDate AND RetroRegister.RetroStatementDate &lt;= @InvoiceEndDate
AND ISNULL(RetroRegister.CedantId, 0) IN (@CedantId)
AND ISNULL(RetroRegister.RetroPartyId, 0) IN (@RetroPartyId)
AND ISNULL(RetroRegister.TreatyCodeId, 0) IN (@TreatyCodeId)
AND ISNULL(RetroRegister.TreatyType, 'NULL') IN (@TreatyType)
AND ISNULL(RetroRegister.RiskQuarter, 'NULL') IN (@RiskQuarter)
AND ISNULL(RetroRegister.OriginalSoaQuarter, 'NULL') IN (@SoaQuarter)
AND RetroRegister.RetroStatementType IN (@InvoiceType)
AND ISNULL(RetroRegister.Frequency, 'NULL') IN (@Frequency)
AND RetroRegister.ReportingType &lt;&gt; 1
GROUP BY RetroRegister.RiskQuarter, RetroRegister.TreatyType, RetroRegister.RetroStatementDate
ORDER BY RetroRegister.RiskQuarter</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
