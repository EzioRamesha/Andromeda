  <DataSources>
    <DataSource Name="InvoiceRegisterDataSource">
      <DataSourceReference>/AndromedaDataSource</DataSourceReference>
      <rd:SecurityType>DataBase</rd:SecurityType>
      <rd:DataSourceID>eab8972e-c22a-4de5-948b-97c8eca1136d</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="InvoiceRegisterSoaData">
      <Query>
        <DataSourceName>InvoiceRegisterDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@TypeId">
            <Value>=Parameters!TypeId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportingQuarter">
            <Value>=Parameters!ReportingQuarter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TreatyType">
            <Value>=Parameters!TreatyType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
  TypeId
  ,CutOffId
  ,TreatyCode
  ,TreatyType
  ,RiskQuarter
  ,SUM(ISNULL(FirstPremium, 0)) AS FirstPremium
  ,SUM(ISNULL(RenewalPremium, 0)) AS RenewalPremium
  ,SUM(ISNULL(AltPremium, 0)) AS AltPremium
  ,SUM(ISNULL(RiskPremium, 0)) AS RiskPremium
FROM
(
SELECT
	1 AS TypeId
	,InvoiceRegisterHistories.CutOffId
	,TreatyCodes.Code AS TreatyCode
	,PickListDetails.Code AS TreatyType
	,InvoiceRegisterHistories.RiskQuarter
	,ISNULL(InvoiceRegisterHistories.Gross1st, 0) AS FirstPremium
	,ISNULL(InvoiceRegisterHistories.GrossRenewal, 0) AS RenewalPremium
	,ISNULL(InvoiceRegisterHistories.AltPremium, 0) AS AltPremium
	,ISNULL(InvoiceRegisterHistories.RiskPremium, 0) AS RiskPremium
FROM
	InvoiceRegisterHistories
INNER JOIN TreatyCodes ON InvoiceRegisterHistories.TreatyCodeId = TreatyCodes.Id
LEFT JOIN PickListDetails ON PickListDetails.Id = TreatyCodes.TreatyTypePickListDetailId
UNION ALL
SELECT
  2 AS TypeId
  ,sdh.CutOffId
  ,sdh.TreatyCode
  ,sdh.TreatyType
  ,sdh.RiskQuarter
  ,SUM(ISNULL(sdh.FirstPremium, 0)) AS FirstPremium
  ,SUM(ISNULL(sdh.RenewalPremium, 0)) AS RenewalPremium
  ,SUM(ISNULL(sdh.AltPremium, 0)) AS AltPremium
  ,SUM(ISNULL(sdh.RiskPremium, 0)) AS RiskPremium
FROM
  (
	SELECT
		SoaDataHistories.CutOffId
                ,SoaDataHistories.SoaDataBatchId
		,SoaDataHistories.TreatyCode
		,SoaDataHistories.TreatyType
		,SoaDataHistories.RiskQuarter
		,ISNULL(SoaDataHistories.NbPremium, 0) AS FirstPremium
		,ISNULL(SoaDataHistories.RnPremium, 0) AS RenewalPremium
		,ISNULL(SoaDataHistories.AltPremium, 0) AS AltPremium
		,ISNULL(SoaDataHistories.RiskPremium, 0) AS RiskPremium
	FROM
	  SoaDataHistories
	INNER JOIN SoaDataBatchHistories ON SoaDataBatchHistories.SoaDataBatchId = SoaDataHistories.SoaDataBatchId AND SoaDataBatchHistories.CutOffId = SoaDataHistories.CutOffId
	WHERE SoaDataHistories.CurrencyCode = 'MYR'
	AND SoaDataBatchHistories.InvoiceStatus = 0
	AND SoaDataBatchHistories.Status &lt;&gt; 255
  ) sdh  
  GROUP BY sdh.RiskQuarter, sdh.TreatyCode, sdh.TreatyType, sdh.CutOffId
) AS Premiums
WHERE Premiums.TypeId = @TypeId
AND Premiums.CutOffId = @ReportingQuarter
AND Premiums.TreatyType IN (@TreatyType)
GROUP BY Premiums.TypeId, Premiums.RiskQuarter, Premiums.TreatyCode, Premiums.TreatyType, Premiums.CutOffId
ORDER BY Premiums.RiskQuarter</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="TypeId">
          <DataField>TypeId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CutOffId">
          <DataField>CutOffId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="RiskQuarter">
