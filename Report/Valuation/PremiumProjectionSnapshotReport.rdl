    <DataSet Name="TreatyTypeLookup">
      <Query>
        <DataSourceName>InvoiceRegisterDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@TypeId">
            <Value>=Parameters!TypeId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  t.[Value]
  ,t.[Label]
FROM (	
	SELECT DISTINCT
  	   1 AS TypeId
  	   ,PickListDetails.Code AS [Value]
  	   ,PickListDetails.Code AS [Label]
	FROM
  	InvoiceRegisterHistories
	INNER JOIN TreatyCodes ON TreatyCodes.Id = InvoiceRegisterHistories.TreatyCodeId
	INNER JOIN PickListDetails ON PickListDetails.Id = TreatyCodes.TreatyTypePickListDetailId
	UNION
	SELECT DISTINCT
    	   2 AS TypeId
    	   ,ISNULL(SoaDataHistories.TreatyType, 'NULL') AS [Value]
    	   ,ISNULL(SoaDataHistories.TreatyType, '(Blank)') AS [Label]
	FROM SoaDataHistories
        INNER JOIN SoaDataBatchHistories ON SoaDataBatchHistories.SoaDataBatchId = SoaDataHistories.SoaDataBatchId AND SoaDataBatchHistories.CutOffId = SoaDataHistories.CutOffId
	WHERE SoaDataHistories.CurrencyCode = 'MYR'
	AND SoaDataBatchHistories.InvoiceStatus = 0
	AND SoaDataBatchHistories.Status &lt;&gt; 255
)t
WHERE t.TypeId = @TypeId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Value">
          <DataField>Value</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Label">
          <DataField>Label</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CutOffLookup">
      <Query>
        <DataSourceName>InvoiceRegisterDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@TypeId">
            <Value>=Parameters!TypeId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF @TypeId = 1
	SELECT DISTINCT
		InvoiceRegisterHistories.CutOffId AS [Value]
		,CutOff.Quarter AS [Label]
	FROM InvoiceRegisterHistories
	INNER JOIN CutOff ON CutOff.Id = InvoiceRegisterHistories.CutOffId
	WHERE CutOff.Month IN (3,6,9,12)
	AND CutOff.Status = 4
ELSE
	SELECT DISTINCT
		SoaDataHistories.CutOffId AS [Value]
		,CutOff.Quarter AS [Label]
	FROM SoaDataHistories
	INNER JOIN CutOff ON CutOff.Id = SoaDataHistories.CutOffId
	WHERE CutOff.Month IN (3,6,9,12)
	AND CutOff.Status = 4</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Value">
          <DataField>Value</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Label">
          <DataField>Label</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
